  <button @click=\"toggleNotifications\" class=\"notification-button\">\n        <i class=\"bx bx-bell\"></i>\n      </button>\n      <div v-if=\"showNotifications\" class=\"notification-modal\">\n        <p>Notificaciones:</p>\n        <ul>\n          <li>Nueva canción agregada</li>\n          <li>Actualización de perfil exitosa</li>\n        </ul>\n      </div>\n    </div>\n\n    <!-- Contenido Original -->\n    <ProtectedNavbar />\n    <div class=\"content\">\n      <div class=\"header\">\n        <h1>Canción</h1>\n        <div id=\"app\">\n          <button @click=\"showCreateModal = true\">Crear Canción</button>\n          <MyModal :isVisible=\"showCreateModal\" @close=\"showCreateModal = false\">\n            <form v-if=\"!isEditing\" @submit.prevent=\"handleCreate\">\n              <h2>Crear Canción</h2>\n              <div class=\"form-group\">\n                <label for=\"songName\">Nombre de la Canción:</label>\n                <input type=\"text\" v-model=\"formData.songName\" required />\n              </div>\n            </form>\n          </MyModal>\n        </div>\n      </div>\n      <div class=\"table-container\">\n        <table>\n          <thead>\n            <tr>\n              <th>#</th>\n              <th>Foto</th>\n              <th>Nombre de la Canción</th>\n              <th>Nombre del Artista</th>\n              <th>Álbum</th>\n              <th>Estado</th>\n              <th>Acciones</th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr v-for=\"(song, index) in filteredSongs\" :key=\"index\">\n              <td>{{ index + 1 }}</td>\n              <td><img :src=\"song.photo\" alt=\"Foto\" class=\"song-photo\" /></td>\n              <td>{{ song.songName }}</td>\n              <td>{{ song.artistName }}</td>\n              <td>{{ song.album }}</td>\n              <td>{{ song.status }}</td>\n              <td>\n                <button @click=\"viewSongDetails(song)\">Ver</button>\n                <button @click=\"startEditing(song)\">Editar</button>\n              </td>\n            </tr>\n          </tbody>\n        </table>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport ProtectedNavbar from \"../components/ProtectedNavbar.vue\";\nimport MyModal from \"../components/Modal.vue\";\n\nexport default {\n  components: {\n    ProtectedNavbar,\n    MyModal,\n  },\n  data() {\n    return {\n      showCreateModal: false,\n      showNotifications: false, // Estado de notificaciones\n      formData: {\n        songName: \"\",\n        artistName: \"\",\n        album: \"\",\n        status: \"Activo\",\n        photo: \"\",\n      },\n      songs: [\n        {\n          photo: \"https://i.pinimg.com/736x/48/22/bf/4822bf54ee339f9e3842192de95a906e.jpg\",\n          songName: \"Canción 1\",\n          artistName: \"Artista 1\",\n          album: \"Álbum 1\",\n          status: \"Activo\",\n        },\n      ],\n    };\n  },\n  computed: {\n    filteredSongs() {\n      return this.songs.filter((song) =>\n        song.songName.toLowerCase().includes(this.searchQuery?.toLowerCase() || \"\")\n      );\n    },\n  },\n  methods: {\n    toggleNotifications() {\n      this.showNotifications = !this.showNotifications;\n    },\n    viewSongDetails(song) {\n      alert(`Detalles de: ${song.songName}`);\n    },\n    startEditing(song) {\n      this.formData = { ...song };\n      this.showCreateModal = true;\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Estilos específicos para la barra superior */\n.top-bar {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  background-color: #f8f9fa;\n  padding: 10px 20px;\n  border-bottom: 1px solid #ddd;\n}\n\n.avatar {\n  width: 40px;\n  height: 40px;\n  border-radius: 50%;\n}\n\n.username {\n  margin-left: 10px;\n  font-weight: bold;\n}\n\n.notification-button {\n  background: none;\n  border: none;\n  cursor: pointer;\n  font-size: 20px;\n}\n\n.notification-modal {\n  position: absolute;\n  top: 50px;\n  right: 10px;\n  background: white;\n  border: 1px solid #ccc;\n  padding: 10px;\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\n}\n\n.notification-modal ul {\n  list-style: none;\n  margin: 0;\n  padding: 0;\n}\n\n.notification-modal li {\n  margin-bottom: 5px;\n}\n</style>\n"],"mappings":"AA0EA,OAAOA,eAAc,MAAO,mCAAmC;AAC/D,OAAOC,OAAM,MAAO,yBAAyB;AAE7C,eAAe;EACbC,UAAU,EAAE;IACVF,eAAe;IACfC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,eAAe,EAAE,KAAK;MACtBC,iBAAiB,EAAE,KAAK;MAAE;MAC1BC,QAAQ,EAAE;QACRC,QAAQ,EAAE,EAAE;QACZC,UAAU,EAAE,EAAE;QACdC,KAAK,EAAE,EAAE;QACTC,MAAM,EAAE,QAAQ;QAChBC,KAAK,EAAE;MACT,CAAC;MACDC,KAAK,EAAE,CACL;QACED,KAAK,EAAE,yEAAyE;QAChFJ,QAAQ,EAAE,WAAW;QACrBC,UAAU,EAAE,WAAW;QACvBC,KAAK,EAAE,SAAS;QAChBC,MAAM,EAAE;MACV,CAAC;IAEL,CAAC;EACH,CAAC;EACDG,QAAQ,EAAE;IACRC,aAAaA,CAAA,EAAG;MACd,OAAO,IAAI,CAACF,KAAK,CAACG,MAAM,CAAEC,IAAI,IAC5BA,IAAI,CAACT,QAAQ,CAACU,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAACC,WAAW,EAAEF,WAAW,CAAC,KAAK,EAAE,CAC5E,CAAC;IACH;EACF,CAAC;EACDG,OAAO,EAAE;IACPC,mBAAmBA,CAAA,EAAG;MACpB,IAAI,CAAChB,iBAAgB,GAAI,CAAC,IAAI,CAACA,iBAAiB;IAClD,CAAC;IACDiB,eAAeA,CAACN,IAAI,EAAE;MACpBO,KAAK,CAAC,gBAAgBP,IAAI,CAACT,QAAQ,EAAE,CAAC;IACxC,CAAC;IACDiB,YAAYA,CAACR,IAAI,EAAE;MACjB,IAAI,CAACV,QAAO,GAAI;QAAE,GAAGU;MAAK,CAAC;MAC3B,IAAI,CAACZ,eAAc,GAAI,IAAI;IAC7B;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}