{"ast":null,"code":"import ProtectedNavbar from \"../components/ProtectedNavbar.vue\";\nimport MyModal from \"../components/Modal.vue\";\nexport default {\n  components: {\n    ProtectedNavbar,\n    MyModal\n  },\n  data() {\n    return {\n      showCreateModal: false,\n      showNotifications: false,\n      // Modal de notificaciones\n      searchQuery: \"\",\n      formData: {\n        songName: \"\",\n        artistName: \"\",\n        album: \"\",\n        status: \"Activo\",\n        photo: \"\"\n      },\n      songs: [{\n        photo: \"https://i.pinimg.com/736x/48/22/bf/4822bf54ee339f9e3842192de95a906e.jpg\",\n        songName: \"Canción 1\",\n        artistName: \"Artista 1\",\n        album: \"Álbum 1\",\n        status: \"Activo\"\n      }]\n    };\n  },\n  computed: {\n    filteredSongs() {\n      return this.songs.filter(song => song.songName.toLowerCase().includes(this.searchQuery.toLowerCase()));\n    }\n  },\n  methods: {\n    toggleNotifications() {\n      this.showNotifications = !this.showNotifications;\n    },\n    viewSongDetails(song) {\n      alert(`Detalles de: ${song.songName}`);\n    },\n    startEditing(song) {\n      this.formData = {\n        ...song\n      };\n      this.showCreateModal = true;\n    }\n  }\n};","map":{"version":3,"names":["ProtectedNavbar","MyModal","components","data","showCreateModal","showNotifications","searchQuery","formData","songName","artistName","album","status","photo","songs","computed","filteredSongs","filter","song","toLowerCase","includes","methods","toggleNotifications","viewSongDetails","alert","startEditing"],"sources":["C:\\Users\\Julian\\Desktop\\indiec-nuevo\\fronted-vue\\src\\views\\MusicaView.vue"],"sourcesContent":["<template>\n  <div>\n    <!-- Barra Superior -->\n    <header class=\"top-bar\">\n      <div class=\"top-bar-content\">\n        <div class=\"profile-section\">\n          <img\n            src=\"https://i.pinimg.com/736x/48/22/bf/4822bf54ee339f9e3842192de95a906e.jpg\"\n            alt=\"Avatar\"\n            class=\"avatar\"\n          />\n          <span class=\"username\">John Doe</span>\n        </div>\n        <div class=\"notification-section\">\n          <button @click=\"toggleNotifications\" class=\"notification-button\">\n            <i class=\"bx bx-bell\"></i>\n          </button>\n          <div\n            v-if=\"showNotifications\"\n            class=\"notification-modal\"\n          >\n            <h4>Notificaciones</h4>\n            <ul>\n              <li>Nueva canción agregada</li>\n              <li>Actualización de perfil exitosa</li>\n            </ul>\n          </div>\n        </div>\n      </div>\n    </header>\n\n    <!-- Contenido principal -->\n    <ProtectedNavbar />\n    <div class=\"content\">\n      <!-- Tabla de canciones y funcionalidad existente -->\n      <!-- Incluyo todo el contenido sin alteraciones -->\n      <div class=\"header\">\n        <div id=\"capa-padre\">\n          <h1>Canción</h1>\n          <div id=\"app\">\n            <button @click=\"showCreateModal = true\">Crear Canción</button>\n            <MyModal :isVisible=\"showCreateModal\" @close=\"showCreateModal = false\">\n              <!-- Modales de creación y edición -->\n              <form v-if=\"!isEditing\" @submit.prevent=\"handleCreate\">\n                <h2>Crear Canción</h2>\n                <div class=\"form-group\">\n                  <label for=\"songName\">Nombre de la Canción:</label>\n                  <input type=\"text\" v-model=\"formData.songName\" required />\n                </div>\n                <!-- Más campos del formulario... -->\n              </form>\n            </MyModal>\n          </div>\n        </div>\n      </div>\n\n      <!-- Tabla -->\n      <div class=\"table-container\">\n        <table>\n          <thead>\n            <tr>\n              <th>#</th>\n              <th>Foto</th>\n              <th>Nombre de la Canción</th>\n              <th>Nombre del Artista</th>\n              <th>Álbum</th>\n              <th>Estado</th>\n              <th>Acciones</th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr v-for=\"(song, index) in filteredSongs\" :key=\"index\">\n              <td>{{ index + 1 }}</td>\n              <td>\n                <img :src=\"song.photo\" alt=\"Foto\" class=\"song-photo\" />\n              </td>\n              <td>{{ song.songName }}</td>\n              <td>{{ song.artistName }}</td>\n              <td>{{ song.album }}</td>\n              <td>{{ song.status }}</td>\n              <td>\n                <!-- Botones de acciones -->\n                <button @click=\"viewSongDetails(song)\">Ver</button>\n                <button @click=\"startEditing(song)\">Editar</button>\n              </td>\n            </tr>\n          </tbody>\n        </table>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport ProtectedNavbar from \"../components/ProtectedNavbar.vue\";\nimport MyModal from \"../components/Modal.vue\";\n\nexport default {\n  components: {\n    ProtectedNavbar,\n    MyModal,\n  },\n  data() {\n    return {\n      showCreateModal: false,\n      showNotifications: false, // Modal de notificaciones\n      searchQuery: \"\",\n      formData: {\n        songName: \"\",\n        artistName: \"\",\n        album: \"\",\n        status: \"Activo\",\n        photo: \"\",\n      },\n      songs: [\n        {\n          photo: \"https://i.pinimg.com/736x/48/22/bf/4822bf54ee339f9e3842192de95a906e.jpg\",\n          songName: \"Canción 1\",\n          artistName: \"Artista 1\",\n          album: \"Álbum 1\",\n          status: \"Activo\",\n        },\n      ],\n    };\n  },\n  computed: {\n    filteredSongs() {\n      return this.songs.filter((song) =>\n        song.songName.toLowerCase().includes(this.searchQuery.toLowerCase())\n      );\n    },\n  },\n  methods: {\n    toggleNotifications() {\n      this.showNotifications = !this.showNotifications;\n    },\n    viewSongDetails(song) {\n      alert(`Detalles de: ${song.songName}`);\n    },\n    startEditing(song) {\n      this.formData = { ...song };\n      this.showCreateModal = true;\n    },\n  },\n};\n</script>\n\n<style scoped>\n/* Estilos para la barra superior */\n.top-bar {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  background-color: white;\n  padding: 10px 20px;\n  border-bottom: 1px solid #ddd;\n  position: sticky;\n  top: 0;\n  z-index: 1000;\n}\n\n.top-bar-content {\n  display: flex;\n  justify-content: space-between;\n  width: 100%;\n}\n\n.profile-section {\n  display: flex;\n  align-items: center;\n}\n\n.avatar {\n  width: 40px;\n  height: 40px;\n  border-radius: 50%;\n  margin-right: 10px;\n}\n\n.username {\n  font-size: 16px;\n  font-weight: bold;\n}\n\n.notification-section {\n  position: relative;\n}\n\n.notification-button {\n  background: none;\n  border: none;\n  font-size: 20px;\n  cursor: pointer;\n}\n\n.notification-modal {\n  position: absolute;\n  top: 40px;\n  right: 0;\n  background-color: white;\n  border: 1px solid #ddd;\n  padding: 10px;\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\n  z-index: 10;\n}\n\n.notification-modal h4 {\n  margin-bottom: 10px;\n}\n\n.notification-modal ul {\n  list-style: none;\n  padding: 0;\n  margin: 0;\n}\n\n.notification-modal li {\n  margin-bottom: 5px;\n}\n\n/* Estilos generales */\n.table-container {\n  margin-top: 20px;\n}\n\n.song-photo {\n  width: 50px;\n  height: 50px;\n  object-fit: cover;\n}\n</style>\n"],"mappings":"AA8FA,OAAOA,eAAc,MAAO,mCAAmC;AAC/D,OAAOC,OAAM,MAAO,yBAAyB;AAE7C,eAAe;EACbC,UAAU,EAAE;IACVF,eAAe;IACfC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,eAAe,EAAE,KAAK;MACtBC,iBAAiB,EAAE,KAAK;MAAE;MAC1BC,WAAW,EAAE,EAAE;MACfC,QAAQ,EAAE;QACRC,QAAQ,EAAE,EAAE;QACZC,UAAU,EAAE,EAAE;QACdC,KAAK,EAAE,EAAE;QACTC,MAAM,EAAE,QAAQ;QAChBC,KAAK,EAAE;MACT,CAAC;MACDC,KAAK,EAAE,CACL;QACED,KAAK,EAAE,yEAAyE;QAChFJ,QAAQ,EAAE,WAAW;QACrBC,UAAU,EAAE,WAAW;QACvBC,KAAK,EAAE,SAAS;QAChBC,MAAM,EAAE;MACV,CAAC;IAEL,CAAC;EACH,CAAC;EACDG,QA