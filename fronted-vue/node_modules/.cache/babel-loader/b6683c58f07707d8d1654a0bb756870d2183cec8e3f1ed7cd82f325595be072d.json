abel>Nombre:</label>\r\n        <input v-model=\"form.Nombre_disquera\" required />\r\n        \r\n        <label>Descripción:</label>\r\n        <input v-model=\"form.Descripcion_disquera\" required />\r\n        \r\n        <label>URL:</label>\r\n        <input v-model=\"form.Url\" required />\r\n        \r\n        <label>Estado:</label>\r\n        <select v-model=\"form.estado_fk\">\r\n          <option value=\"Activo\">Activo</option>\r\n          <option value=\"Inactivo\">Inactivo</option>\r\n        </select>\r\n        \r\n        <label>Plataforma:</label>\r\n        <select v-model=\"form.plataforma_fk\">\r\n          <option v-for=\"plataforma in plataformas\" :key=\"plataforma.id\" :value=\"plataforma.id\">{{ plataforma.nombre }}</option>\r\n        </select>\r\n        \r\n        <label>Género Musical:</label>\r\n        <select v-model=\"form.genero_musical_fk\">\r\n          <option v-for=\"genero in generos\" :key=\"genero.id\" :value=\"genero.id\">{{ genero.nombre }}</option>\r\n        </select>\r\n        \r\n        <button type=\"submit\">{{ editando ? 'Actualizar' : 'Guardar' }}</button>\r\n        <button type=\"button\" @click=\"cerrarModal\">Cancelar</button>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      disqueras: [],\r\n      plataformas: [],\r\n      generos: [],\r\n      mostrarModal: false,\r\n      editando: false,\r\n      form: {\r\n        Nombre_disquera: '',\r\n        Descripcion_disquera: '',\r\n        Url: '',\r\n        estado_fk: 'Activo',\r\n        plataforma_fk: '',\r\n        genero_musical_fk: ''\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    async obtenerDisqueras() {\r\n      const response = await fetch('/api/disqueras');\r\n      this.disqueras = await response.json();\r\n    },\r\n    async obtenerPlataformas() {\r\n      const response = await fetch('/api/plataformas');\r\n      this.plataformas = await response.json();\r\n    },\r\n    async obtenerGeneros() {\r\n      const response = await fetch('/api/generos');\r\n      this.generos = await response.json();\r\n    },\r\n    async guardarDisquera() {\r\n      const method = this.editando ? 'PUT' : 'POST';\r\n      const url = this.editando ? `/api/disqueras/${this.form.id}` : '/api/disqueras';\r\n      await fetch(url, {\r\n        method,\r\n        headers: { 'Content-Type': 'application/json' },\r\n        body: JSON.stringify(this.form)\r\n      });\r\n      this.mostrarModal = false;\r\n      this.obtenerDisqueras();\r\n    },\r\n    editarDisquera(disquera) {\r\n      this.form = { ...disquera };\r\n      this.editando = true;\r\n      this.mostrarModal = true;\r\n    },\r\n    async eliminarDisquera(id) {\r\n      await fetch(`/api/disqueras/${id}`, { method: 'DELETE' });\r\n      this.obtenerDisqueras();\r\n    },\r\n    cerrarModal() {\r\n      this.mostrarModal = false;\r\n      this.form = {\r\n        Nombre_disquera: '',\r\n        Descripcion_disquera: '',\r\n        Url: '',\r\n        estado_fk: 'Activo',\r\n        plataforma_fk: '',\r\n        genero_musical_fk: ''\r\n      };\r\n      this.editando = false;\r\n    }\r\n  },\r\n  mounted() {\r\n    this.obtenerDisqueras();\r\n    this.obtenerPlataformas();\r\n    this.obtenerGeneros();\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n#capa-padre {\r\n  background-color: aliceblue;\r\n  border-radius: 20px;\r\n  text-align: center;\r\n  margin: 5px auto;\r\n  padding: 10px;\r\n  box-shadow: 5px 2px 3px 1px rgba(0, 0, 0, 0.164);\r\n  font-size: 16px;\r\n  color: black;\r\n}\r\n\r\n#app {\r\n  text-align: center;\r\n  margin-top: 10px;\r\n}\r\n\r\nbutton {\r\n  padding: 10px;\r\n  font-size: 16px;\r\n  cursor: pointer;\r\n  background-color: #0aa5a9;\r\n  color: white;\r\n  border-radius: 13px;\r\n  text-transform: capitalize;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #067b80;\r\n}\r\n\r\nform {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-