{"ast":null,"code":"export default {\n  data() {\n    return {\n      disqueras: [],\n      plataformas: [],\n      generos: [],\n      mostrarModal: false,\n      editando: false,\n      form: {\n        Nombre_disquera: '',\n        Descripcion_disquera: '',\n        Url: '',\n        estado_fk: 'Activo',\n        plataforma_fk: '',\n        genero_musical_fk: ''\n      }\n    };\n  },\n  methods: {\n    async obtenerDisqueras() {\n      const response = await fetch('/api/disqueras');\n      this.disqueras = await response.json();\n    },\n    async obtenerPlataformas() {\n      const response = await fetch('/api/plataformas');\n      this.plataformas = await response.json();\n    },\n    async obtenerGeneros() {\n      const response = await fetch('/api/generos');\n      this.generos = await response.json();\n    },\n    async guardarDisquera() {\n      const method = this.editando ? 'PUT' : 'POST';\n      const url = this.editando ? `/api/disqueras/${this.form.id}` : '/api/disqueras';\n      await fetch(url, {\n        method,\n        headers: {\n          'Content-Type': 'application/json'\n        },\n        body: JSON.stringify(this.form)\n      });\n      this.mostrarModal = false;\n      this.obtenerDisqueras();\n    },\n    editarDisquera(disquera) {\n      this.form = {\n        ...disquera\n      };\n      this.editando = true;\n      this.mostrarModal = true;\n    },\n    async eliminarDisquera(id) {\n      await fetch(`/api/disqueras/${id}`, {\n        method: 'DELETE'\n      });\n      this.obtenerDisqueras();\n    },\n    cerrarModal() {\n      this.mostrarModal = false;\n      this.form = {\n        Nombre_disquera: '',\n        Descripcion_disquera: '',\n        Url: '',\n        estado_fk: 'Activo',\n        plataforma_fk: '',\n        genero_musical_fk: ''\n      };\n      this.editando = false;\n    }\n  },\n  mounted() {\n    this.obtenerDisqueras();\n    this.obtenerPlataformas();\n    this.obtenerGeneros();\n  }\n};","map":{"version":3,"nam