{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport ProtectedNavbar from \"../components/ProtectedNavbar.vue\";\nimport MyModal from \"../components/Modal.vue\";\nimport Swal from \"sweetalert2\";\nexport default {\n  components: {\n    ProtectedNavbar,\n    MyModal\n  },\n  data() {\n    return {\n      showCreateModal: false,\n      searchQuery: \"\",\n      formData: {\n        artistName: \"\",\n        recordlabelName: \"\",\n        genreName: \"\",\n        descriptiontName: \"\",\n        platform: \"\",\n        url: \"\",\n        status: \"Activo\",\n        photo: \"\"\n      },\n      songs: [],\n      isEditing: false,\n      editIndex: null,\n      imagePreview: null,\n      editImagePreview: null\n    };\n  },\n  computed: {\n    filteredSongs() {\n      const query = this.searchQuery.toLowerCase();\n      return this.songs.filter(song => song.artistName.toLowerCase().includes(query));\n    }\n  },\n  methods: {\n    handleCreate() {\n      this.songs.push({\n        ...this.formData\n      });\n      this.showCreateModal = false;\n      this.resetFormData();\n      Swal.fire(\"¡Éxito!\", \"Grupo musical creado.\", \"success\");\n    },\n    startEditing(song) {\n      this.isEditing = true;\n      this.formData = {\n        ...song\n      };\n      this.showCreateModal = true;\n    }\n  }\n};","map":{"version":3,"names":["ProtectedNavbar","MyModal","Swal","components","data","showCreateModal","searchQuery","formData","artistName","recordlabelName","genreName","descriptiontName","platform","url","status","photo","songs","isEditing","editIndex","imagePreview","editImagePreview","computed","filteredSongs","query","toLowerCase","filter","song","includes","methods","handleCreate","push","resetFormData","fire","startEditing"],"sources":["C:\\Users\\canec\\OneDrive\\Escritorio\\indiec-nuevo\\fronted-vue\\src\\views\\GrupoMusicalView.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <ProtectedNavbar />\r\n\r\n    <div class=\"content\">\r\n      <div class=\"header\">\r\n        <div id=\"capa-padre\">\r\n          <h1>Grupo Musical</h1>\r\n          <div id=\"app\">\r\n            <button @click=\"showCreateModal = true\">Crear Grupo</button>\r\n            <MyModal :isVisible=\"showCreateModal\" @close=\"showCreateModal = false\">\r\n              <form v-if=\"!isEditing\" @submit.prevent=\"handleCreate\">\r\n                <h2>Crear Grupo</h2>\r\n\r\n                <div class=\"form-group custom-form-group\">\r\n                  <label for=\"imagen\" class=\"upload-label custom-upload-label\">\r\n                    <i class=\"bx bx-check\"></i> Subir Imagen\r\n                  </label>\r\n                  <input\r\n                    type=\"file\"\r\n                    id=\"imagen\"\r\n                    class=\"custom-upload-input\"\r\n                    @change=\"handleFileUpload\"\r\n                    accept=\"image/*\"\r\n                  />\r\n                </div>\r\n\r\n                <div v-if=\"imagePreview\" class=\"image-preview custom-image-preview\">\r\n                  <img :src=\"imagePreview\" alt=\"Vista previa de la imagen\" class=\"custom-preview-img\" />\r\n                </div>\r\n\r\n                <div class=\"form-group\">\r\n                  <label for=\"artistName\">Nombre del Artista:</label>\r\n                  <input type=\"text\" v-model=\"formData.artistName\" required placeholder=\"Ingrese el nombre del artista\" />\r\n                </div>\r\n\r\n                <div class=\"form-group\">\r\n                  <label for=\"recordlabelName\">Grupo:</label>\r\n                  <input type=\"text\" v-model=\"formData.recordlabelName\" required placeholder=\"Ingrese el nombre del grupo\" />\r\n                </div>\r\n\r\n                <div class=\"form-group\">\r\n                  <label for=\"genreName\">Género Musical:</label>\r\n                  <input type=\"text\" v-model=\"formData.genreName\" required placeholder=\"Ingrese el género musical\" />\r\n                </div>\r\n\r\n                <div class=\"form-group\">\r\n                  <label for=\"descriptiontName\">Descripción:</label>\r\n                  <input type=\"text\" v-model=\"formData.descriptiontName\" required placeholder=\"Ingrese descripción\" />\r\n                </div>\r\n\r\n                <div class=\"form-group\">\r\n                  <label for=\"platform\">Plataforma:</label>\r\n                  <input type=\"text\" v-model=\"formData.platform\" required />\r\n                </div>\r\n\r\n                <div class=\"form-group\">\r\n                  <label for=\"url\">URL:</label>\r\n                  <input type=\"url\" v-model=\"formData.url\" required placeholder=\"Ingrese la URL\" />\r\n                </div>\r\n\r\n                <div class=\"button-container\">\r\n                  <button type=\"submit\">Guardar</button>\r\n                </div>\r\n              </form>\r\n\r\n              <form v-else @submit.prevent=\"handleEdit\">\r\n                <h2>Editar Grupo Musical</h2>\r\n\r\n                <div class=\"form-group custom-form-group\">\r\n                  <label for=\"edit-imagen\" class=\"upload-label custom-upload-label\">\r\n                    <i class=\"bx bx-check\"></i> Cambiar Imagen\r\n                  </label>\r\n                  <input\r\n                    type=\"file\"\r\n                    id=\"edit-imagen\"\r\n                    class=\"custom-upload-input\"\r\n                    @change=\"handleEditFileUpload\"\r\n                    accept=\"image/*\"\r\n                  />\r\n                </div>\r\n\r\n                <div v-if=\"editImagePreview\" class=\"image-preview custom-image-preview\">\r\n                  <img :src=\"editImagePreview\" alt=\"Vista previa de la imagen\" class=\"custom-preview-img\" />\r\n                </div>\r\n\r\n                <div class=\"form-group\">\r\n                  <label for=\"artistName\">Nombre del Artista:</label>\r\n                  <input type=\"text\" v-model=\"formData.artistName\" required placeholder=\"Ingrese el nombre del artista\" />\r\n                </div>\r\n\r\n                <div class=\"form-group\">\r\n                  <label for=\"recordlabelName\">Grupo:</label>\r\n                  <input type=\"text\" v-model=\"formData.recordlabelName\" required placeholder=\"Ingrese el nombre del grupo\" />\r\n                </div>\r\n\r\n                <div class=\"form-group\">\r\n                  <label for=\"genreName\">Género Musical:</label>\r\n                  <input type=\"text\" v-model=\"formData.genreName\" required placeholder=\"Ingrese el género musical\" />\r\n                </div>\r\n\r\n                <div class=\"form-group\">\r\n                  <label for=\"descriptiontName\">Descripción:</label>\r\n                  <input type=\"text\" v-model=\"formData.descriptiontName\" required placeholder=\"Ingrese descripción\" />\r\n                </div>\r\n\r\n                <div class=\"form-group\">\r\n                  <label for=\"platform\">Plataforma:</label>\r\n                  <input type=\"text\" v-model=\"formData.platform\" required />\r\n                </div>\r\n\r\n                <div class=\"form-group\">\r\n                  <label for=\"url\">URL:</label>\r\n                  <input type=\"url\" v-model=\"formData.url\" required placeholder=\"Ingrese la URL\" />\r\n                </div>\r\n\r\n                <div class=\"button-container\">\r\n                  <button type=\"submit\">Guardar Cambios</button>\r\n                </div>\r\n              </form>\r\n            </MyModal>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"table-container\">\r\n        <table>\r\n          <thead>\r\n            <tr>\r\n              <th>#</th>\r\n              <th>Foto</th>\r\n              <th>Nombre del Grupo</th>\r\n              <th>Nombre del Artista</th>\r\n              <th>Género Musical</th>\r\n              <th>Descripción</th>\r\n              <th>Plataforma</th>\r\n              <th>URL</th>\r\n              <th>Estado</th>\r\n              <th>Acciones</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"(song, index) in filteredSongs\" :key=\"index\">\r\n              <td>{{ index + 1 }}</td>\r\n              <td><img :src=\"song.photo\" alt=\"Foto\" class=\"song-photo\" /></td>\r\n              <td>{{ song.recordlabelName }}</td>\r\n              <td>{{ song.artistName }}</td>\r\n              <td>{{ song.genreName }}</td>\r\n              <td>{{ song.descriptiontName }}</td>\r\n              <td>{{ song.platform }}</td>\r\n              <td><a :href=\"song.url\" target=\"_blank\">{{ song.url }}</a></td>\r\n              <td><span :class=\"getSongStatusClass(song.status)\">{{ song.status }}</span></td>\r\n              <td>\r\n                <button class=\"btn view-btn\" @click=\"viewSongDetails(song)\"><i class=\"bx bx-show\"></i></button>\r\n                <button class=\"btn edit-btn\" @click=\"startEditing(song)\"><i class=\"bx bx-edit\"></i></button>\r\n                <button v-if=\"song.status === 'Activo'\" class=\"btn delete-btn\" @click=\"deleteSong(song)\">\r\n                  <i class=\"bx bx-trash\"></i>\r\n                </button>\r\n                <button v-else class=\"btn restore-btn\" @click=\"restoreSong(song)\">\r\n                  <i class=\"bx bx-undo\"></i>\r\n                </button>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport ProtectedNavbar from \"../components/ProtectedNavbar.vue\";\r\nimport MyModal from \"../components/Modal.vue\";\r\nimport Swal from \"sweetalert2\";\r\n\r\nexport default {\r\n  components: { ProtectedNavbar, MyModal },\r\n  data() {\r\n    return {\r\n      showCreateModal: false,\r\n      searchQuery: \"\",\r\n      formData: {\r\n        artistName: \"\",\r\n        recordlabelName: \"\",\r\n        genreName: \"\",\r\n        descriptiontName: \"\",\r\n        platform: \"\",\r\n        url: \"\",\r\n        status: \"Activo\",\r\n        photo: \"\"\r\n      },\r\n      songs: [],\r\n      isEditing: false,\r\n      editIndex: null,\r\n      imagePreview: null,\r\n      editImagePreview: null,\r\n    };\r\n  },\r\n  computed: {\r\n    filteredSongs() {\r\n      const query = this.searchQuery.toLowerCase();\r\n      return this.songs.filter(song => song.artistName.toLowerCase().includes(query));\r\n    },\r\n  },\r\n  methods: {\r\n    handleCreate() {\r\n      this.songs.push({ ...this.formData });\r\n      this.showCreateModal = false;\r\n      this.resetFormData();\r\n      Swal.fire(\"¡Éxito!\", \"Grupo musical creado.\", \"success\");\r\n    },\r\n    startEditing(song) {\r\n      this.isEditing = true;\r\n      this.formData = { ...song };\r\n      this.showCreateModal = true;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n#capa-padre {\r\n  background-color: aliceblue;\r\n  border-radius: 20px;\r\n  text-align: center;\r\n  margin: 5px auto;\r\n  padding: 10px;\r\n  box-shadow: 5px 2px 3px 1px rgba(0, 0, 0, 0.164);\r\n  font-size: 16px;\r\n  color: black;\r\n}\r\n\r\n#app {\r\n  text-align: center;\r\n  margin-top: 10px;\r\n}\r\n\r\nbutton {\r\n  padding: 10px;\r\n  font-size: 16px;\r\n  cursor: pointer;\r\n  background-color: #0aa5a9;\r\n  color: white;\r\n  border-radius: 13px;\r\n  text-transform: capitalize;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #067b80;\r\n}\r\n\r\nform {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n\r\n.form-group {\r\n  display: flex;\r\n  flex-direction: row;\r\n  align-items: center;\r\n  margin-bottom: 15px;\r\n}\r\n\r\n.form-group label {\r\n  flex: 1;\r\n  margin-right: 10px;\r\n  font-weight: bold;\r\n  text-align: right;\r\n}\r\n\r\n.form-group input {\r\n  flex: 2;\r\n  padding: 5p